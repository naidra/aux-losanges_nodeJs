  <main id="main" class="main">

  <%
    if (locals.flashes) {
      const categories = Object.keys(locals.flashes);
  %>
    <div class="pt-3 pb-2">
      <%
        for (var i = 0; i < categories.length; i++ ) {
          const category = categories[i];
          const message = flashes[category];
          for (var j = 0; j < message.length; j++ ) {
      %>
            <div class="flash flash--<%- category %> d-flex justify-content-end align-items-center border border-<%- category %> rounded">
              <p class="flash__text m-0 pe-3"><%- message %></p>
              <button class="flash__remove border-0 bg-<%- category %> opacity-75 py-2 px-3 fw-bold" onClick="this.parentElement.remove()">&times;</button>
            </div>
      <%
          }
        }
      %>
    </div>
  <%
    }
